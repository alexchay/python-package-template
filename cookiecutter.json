{
    "full_name": "Buzz Lightyear",
    "email": "bl@example.com",
    "github_username": "gh_username",
    "project_name": "python-package",
    "project_slug": "{{ cookiecutter.project_name.lower().replace(' ', '_').replace('-', '_') }}",
    "project_description": "Python Boilerplate contains all the boilerplate you need to create a Python package.",
    "pypi_username": "{{ cookiecutter.github_username }}",
    "versioning": [
        "semver",
        "calver"
    ],
    "version": "{% if cookiecutter.versioning == 'calver' %}2023.01.01.0{% else %}0.1.0{% endif %}",
    "add_vscode_settings": false,
    "use_gotask": false,
    "dependency_management": [
        "poetry",
        "pip"
    ],
    "use_pytest": true,
    "use_mkdocs": true,
    "use_pylint": false,
    "use_ruff": true,
    "use_flake8": false,
    "use_mypy": false,
    "use_black": true,
    "use_tox": false,
    "use_pre_commit": true,
    "check_commit_syntax": true,
    "minimal_python_version": [
        3.8,
        3.9,
        3.10
    ],
    "max_line_length": 120,
    "_copy_without_render": [
        "taskfiles"
    ]
}